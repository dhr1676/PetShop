{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "max-line-length": [true, 200],
    "no-empty-interface": false,
    "no-console": false,
    "ordered-imports": false
  }
}